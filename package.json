{
  "name": "innoq-styleguide",
  "version": "0.5.36",
  "description": "INNOQ's Styleguide and Component Library",
  "license": "Copyright 2017 INNOQ Deutschland GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/innoq/innoq-styleguide.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel server-watch compile-watch",
    "server": "fractal start",
    "server-watch": "nodemon -I -w components -e html -e jsx -x 'npm run server'",
    "compile": "faucet",
    "compile-watch": "npm run compile -- --watch",
    "test": "eslint --cache --ext .js --ext .jsx . && sass-lint -v -q",
    "site": "yarn run compile && fractal build",
    "dist": "./scripts/dist",
    "release": "release-it"
  },
  "dependencies": {
    "modularscale-sass": "^3.0.3"
  },
  "devDependencies": {
    "complate-fractal": "^0.4.2",
    "document-register-element": "^1.7.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "faucet-pipeline": "^1.0.0-beta.0",
    "faucet-pipeline-js": "^1.0.0-beta.0",
    "faucet-pipeline-sass": "^1.0.0-beta.0",
    "faucet-pipeline-static": "^1.0.0-beta.0",
    "nodemon": "^1.14.1",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "release-it": "^5.0.0",
    "sass-lint": "^1.12.0"
  }
}
